@model FinancialPortal.Models.Households

@{
    ViewBag.Title = "Index";
}

<h4>Members</h4>
    <table class="table">
        <tr>
            
            <th>
                @Html.DisplayName("Name")
            </th>
            <th>
                @Html.DisplayName("Email")
            </th>

        </tr>

        @foreach (var item in Model.ApplicationUsers)
        {
            <tr>
                
                <td>
                    @Html.DisplayFor(modelItem => item.DisplayName)
                </td>
                <td>
                    @Html.DisplayFor(modeItem => item.Email)
                </td>

            </tr>
        }

    </table>